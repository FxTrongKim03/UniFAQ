{
  "name": "unifaq",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapping": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "components/**/*.js",
      "hooks/**/*.js",
      "*.js",
      "!**/*.test.js",
      "!server.js",
      "!.eslintrc.js"
    ]
  },
  "dependencies": {
    "finalhandler": "^1.2.0",
    "serve-static": "^1.15.0",
    "sprintf-js": "^1.1.3"
  }
}
